{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && yarn fk_fix",
    "fk_fix": "if which gsed > /tmp/test.txt; then gsed -i s,://,:xs, node_modules/@formkit/pro/index.mjs && gsed -i s,://,:xs, node_modules/@formkit/pro/index.cjs; else sed -i s,://,:xs, node_modules/@formkit/pro/index.mjs && sed -i s,://,:xs, node_modules/@formkit/pro/index.cjs; fi",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:prettier",
    "lintfix": "prettier --write --list-different ."
  },
  "dependencies": {
    "@formkit/icons": "^1.5.9",
    "@formkit/nuxt": "^1.5.9",
    "@formkit/pro": "^0.122.0",
    "@heroicons/vue": "^2.1.1",
    "@influxdata/influxdb-client": "^1.33.2",
    "ansicolor": "^2.0.1",
    "balena-sdk": "^19.5.4",
    "delaunator": "^5.0.1",
    "highcharts": "^11.3.0",
    "highcharts-vue": "^1.4.3",
    "nuxt": "^3.10.1",
    "three": "^0.161.0",
    "tweakpane": "^4.0.3",
    "vue": "^3.4.15",
    "vue-final-modal": "^4.5.4",
    "vue-router": "^4.2.5",
    "vue-toast-notification": "^3"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/supabase": "^1.1.6",
    "@nuxtjs/tailwindcss": "^6.11.4",
    "@types/three": "^0.161.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nuxt3-leaflet": "^1.0.12",
    "prettier": "^3.2.5",
    "sass": "^1.70.0",
    "supabase": "^1.142.2"
  }
}
